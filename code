<script src="https://unpkg.com/popper.js@1"></script> <script src="https://unpkg.com/tippy.js@5/dist/tippy-bundle.iife.js"></script> <link rel="stylesheet" href="https://unpkg.com/tippy.js@5/dist/svg-arrow.css" />
<!-----------Tooltips Script----------->

<!-----------NPF photosets script by annasthms @ tumblr----------->
<script src="https://static.tumblr.com/0podkko/qWqq8va08/photosets.js"></script>
<!-----------NPF photosets script by annasthms @ tumblr----------->
 
<script>
$(document).ready(function(){
    //NPF photosets script by annasthms
    undoPhotoset({
        'posts': '.posts', 
        'text class': 'caption',
        'photoset class': 'photoset'
    },true); 
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '{select:photoset gutter}px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
    });
        tippy('a[title]', { 
            theme: 'custom', 
            arrow: tippy.roundArrow, 
            zIndex: 9999999999, 
            maxWidth: 300, 
            content(reference) { 
                const title = reference.getAttribute('title'); 
                reference.removeAttribute('title'); return title; }, 
            
        });
        // audio player fix by @ shudesigns
        var $audio = $('iframe.tumblr_audio_player');
        $audio.load(function() {
        $(this).contents().find('head').append('<style type="text/css">' +
      '.audio-player { background: {color:accent}; color: {color:accented text} !important; }' +
      '.audio-player .audio-info .track-artist { color: {color:accented text} !important; }' +
    '</style>');
  });
       //search suggestions script    
    $('.search').submit(function(event){
       var value = $('input:first').val();
       location.replace('https://{Name}.tumblr.com/tagged/' + value);
    });
});
</script>

