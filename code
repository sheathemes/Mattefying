/*----------THEME BASICS----------*/

html, body {
    color: {color:text};
    font-family: {select:Body Font}, helvetica, sans-serif;
    font-weight: 400;
    font-size:{select:font size}px;
    line-height:180%;
    margin:0;
    padding: 0;
    text-align:left;
    background: {color:background} url({image:Background}) center;
    background-attachment: {select:Background Attachment};
    background-repeat: {select:Background Repeat};
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    animation: fadein 2.0s;
    -moz-animation: fadein 2.0s; /* Firefox */
    -webkit-animation: fadein 2.0s; /* Safari and Chrome */
    -o-animation: fadein 2.0s; /* Opera */
}
 
b,strong {
    color:{color:bold};
    font-weight: 700;
}
 
i, em {
    color:{color:italic};
}
 
u {
    color:{color:underline};
}
 
hr {
    width: 30%;
    height: 1px;
    background-color: {color:post borders};
    color: {color:post borders};
    border: 0px solid transparent;
}
 
small,sub,sup {
    font-size: calc({select:font size} - 0.5px);
}
 
h1,h2,h3 {
    font-weight: 700;
    font-size: 2em;
    color: {color:bold};
}
 
h4,h5,h6 {
    font-weight: 700;
    font-size: 1.8em;
    color: {color:bold};
}
 
pre {
    white-space: pre-wrap;
    white-space: -moz-pre-wrap;
    white-space: -pre-wrap;
    white-space: -o-pre-wrap;
    word-wrap: break-word;
